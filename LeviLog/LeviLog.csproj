<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<WarningsAsErrors>true</WarningsAsErrors>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Description>A customizable logger with a web based front end</Description>
		<PackageProjectUrl>https://github.com/TizzyT566/LeviLog</PackageProjectUrl>
		<RepositoryUrl>https://github.com/TizzyT566/LeviLog</RepositoryUrl>
		<FileVersion>0.9.0</FileVersion>
		<AssemblyVersion>0.9.0</AssemblyVersion>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<Version>0.9.0</Version>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\LICENSE.txt">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>
